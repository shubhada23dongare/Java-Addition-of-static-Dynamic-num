package Addition;




public class abc {
	int x;
	 double m1(){
		
		 double x=20.0;
		 int y=20;
		 int result =(int) (x+y);
		return result;
	 }
	 
	 void m2()
	 {
		 int x=20;
		 int y=20;
		 int result =x+y;
		
	 }
	 public static void main(String[] args) {
		Z z=new Z();
		z.m3();
	}
}

class Z{
	void m3()
	{
		abc a=new abc();
		int i=10;
